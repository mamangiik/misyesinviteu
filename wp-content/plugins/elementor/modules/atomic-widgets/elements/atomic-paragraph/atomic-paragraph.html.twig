{% if settings.paragraph is not empty %}
        <p class="{{ settings.classes | merge( [ base_styles.base ] ) | join(' ') }}">
            {% if settings.link.href %}
                <a href="https://misyesinviteu.vercel.app/wp-content/plugins/elementor/modules/atomic-widgets/elements/atomic-paragraph/{{ settings.link.href | e('full_url') }}" target="{{ settings.link.target }}" class="{{ base_styles['link-base'] }}">
                    {{ settings.paragraph }}
                </a>
            {% else %}
                {{ settings.paragraph }}
            {% endif %}
        
{% endif %}
