{% if settings.text is not empty %}
    {% set classes = settings.classes | merge( [ base_styles.base ] ) | join(' ') %}

    {% if settings.link.href %}
        <a href="https://mamangiik.github.io/misyesinviteu/wp-content/plugins/elementor/modules/atomic-widgets/elements/atomic-button/{{ settings.link.href | e('full_url') }}" target="{{ settings.link.target }}" class="{{ classes }}">
            {{ settings.text }}
        </a>
    {% else %}
        <button class="{{ classes }}">
            {{ settings.text }}
        </button>
    {% endif %}
{% endif %}
